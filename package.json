{
  "name": "foodBytes",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "build": "NODE_ENV=production browserify client/index.js -o public/bundle.js -g uglifyify",
    "postinstall": "npm run build",
    "build:css": "node-sass --include-path public/stylesheets --output-style compressed public/stylesheets/main.scss public/stylesheets/main.css",
    "watch": "watchify client/index.js -o public/bundle.js -dv",
    "watch:css": "node-sass --include-path public/stylesheets -w public/stylesheets/main.scss public/stylesheets/main.css",
    "test": "npm run test:unit && npm run test:server && npm run test:intergration",
    "test:server": "node ./helpers/test.js | tap-spec",
    "test:unit": "babel-tape-runner ./client/tests/**/*.js | tap-spec",
    "test:unit-all": "browserify ./client/tests/**/*.js | tape | tap-spec",
    "test:integration": "chimp --watch"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "engines": {
    "node": "6.4.0"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "body-parser": "~1.15.1",
    "browserify": "^13.1.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "deep-freeze": "0.0.1",
    "domready": "^1.0.8",
    "dotenv": "2.0.0",
    "express": "~4.13.4",
    "express-history-api-fallback": "2.0.0",
    "hbs": "~4.0.0",
    "install": "^0.8.1",
    "knex": "0.11.10",
    "latest-version": "^2.0.0",
    "morgan": "~1.7.0",
    "node-sass": "^3.9.3",
    "pg": "6.1.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-player": "0.9.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "serve-favicon": "~2.3.0",
    "superagent": "^2.2.0",
    "uglifyify": "^3.0.2"
  },
  "devDependencies": {
    "babel-tape-runner": "^2.0.1",
    "chimp": "^0.40.6",
    "cucumber-assert": "^1.0.4",
    "expect": "^1.20.2",
    "nodemon": "^1.10.2",
    "react-addons-test-utils": "^15.3.1",
    "supertest": "^2.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "url": "^0.11.0",
    "watchify": "^3.7.0",
    "xtend": "^4.0.1"
  }
}
